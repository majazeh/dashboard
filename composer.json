{
    "name": "majazeh/dashboard",
    "description": "Majazeh laravel dashboard",
    "license": "MIT",
    "authors": [
    {
        "name": "baravak",
        "email": "itb.baravak@gmail.com"
    }],
    "autoload":
    {
        "psr-4":
        {
            "App\\": "src/",
            "Majazeh\\Dashboard\\": "src/"
        },
        "files":["src/helpers.php"]
    },
    "extra":
    {
        "laravel":
        {
            "providers": [
                "Majazeh\\Dashboard\\MajazehDashboardProvider",
                "Majazeh\\Dashboard\\MajazehAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/socialite": "^3.1",
        "doctrine/dbal": "^2.8",
        "laravel/passport": "^7.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2"
    }
}
